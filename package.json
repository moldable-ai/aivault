{
  "name": "aivault",
  "private": true,
  "scripts": {
    "build": "cargo build --all-targets --all-features",
    "build:release": "cargo build --release --all-targets --all-features",
    "dev": "cargo run",
    "devd": "cargo run --bin aivaultd",
    "release:cli": "node scripts/release-cli.js",
    "format": "cargo fmt --all",
    "lint": "cargo clippy --all-targets --all-features -- -D warnings",
    "check-types": "cargo check --all-targets --all-features",
    "test": "cargo test --all-targets --all-features",
    "test:e2e": "cargo test --test e2e_cli_local --test e2e_cli_local_tls --test e2e_cli_invoke",
    "test:e2e:local": "cargo test --test e2e_cli_local --test e2e_cli_local_tls",
    "test:e2e:network": "AIVAULT_E2E_NETWORK=1 cargo test --test e2e_cli_invoke",
    "test:all": "pnpm test && pnpm test:e2e",
    "evidence": "node scripts/evidence-computed.js",
    "trace": "node scripts/evidence-computed.js --trace",
    "trace:json": "node scripts/evidence-computed.js --trace-json",
    "stories": "node scripts/stories.js"
  },
  "license": "Apache-2.0"
}
